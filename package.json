{
  "name": "svelte-websocket-store",
  "version": "0.0.0-semantic-release",
  "publishConfig": {
    "access": "public"
  },
  "main": "./src/index.mjs",
  "svelte": "./src/index.svelte",
  "description": "svelte store with a websocket backend",
  "keywords": [
    "component",
    "store",
    "svelte",
    "web",
    "websocket"
  ],
  "contributors": [
    {
      "name": "Markus Felten",
      "email": "markus.felten@gmx.de"
    }
  ],
  "license": "BSD-2-Clause",
  "scripts": {
    "start": "rollup -c example/rollup.config.mjs -w && rollup -c  -w",
    "test": "npm run test:cafe",
    "docs": "documentation readme ./src/index.mjs --section=API",
    "lint": "documentation lint ./src/index.mjs",
    "test:cafe": "testcafe $BROWSER:headless tests/cafe/*.js -s build/test --app-init-delay 1000 --app \"rollup -c example/rollup.config.mjs -w\" && testcafe $BROWSER:headless tests/cafe/*.js -s build/test --app-init-delay 1000 --app \"rollup -c  -w\""
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^9.0.0",
    "documentation": "^13.0.2",
    "rollup": "^2.26.9",
    "rollup-plugin-dev": "^1.1.2",
    "rollup-plugin-svelte": "^6.0.0",
    "semantic-release": "^17.1.1",
    "svelte": "^3.24.1",
    "testcafe": "^1.9.1",
    "ws": "^7.3.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/arlac77/svelte-websocket-store.git"
  },
  "bugs": {
    "url": "https://github.com/arlac77/svelte-websocket-store/issues"
  },
  "homepage": "https://github.com/arlac77/svelte-websocket-store#readme",
  "template": {
    "inheritFrom": [
      "arlac77/template-arlac77-github",
      "arlac77/template-svelte-component"
    ]
  }
}
